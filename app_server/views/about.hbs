<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>{{title}}</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			<div id="header">
                {{> header}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							{{#each aboutData.body}}
							<h1>{{this.name}}</h1>
							<h2>{{this.title}}</h2>
							{{{this.description}}}
							{{/each}}
							<hr>
							<div class="ads">
								{{#each aboutData.ads}}
								<h2>{{this.name}}</h2>
								{{{this.description}}}
								{{/each}}
							</div>
							{{#each aboutData.sidebar}}
							<h2>{{this.name}}</h2>
							{{{this.description}}}
							{{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
        {{> footer}}
	</div>
</body>
</html>